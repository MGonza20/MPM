            "src/**/*.{js,jsx}",
            "!src/index.js",
            "!src/App.js",
            "!src/app/store.js"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!axios)",
            "node_modules/react-leaflet"
        ]
